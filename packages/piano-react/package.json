{
  "name": "@stromland/piano-react",
  "version": "0.4.3",
  "private": true,
  "homepage": "http://stromland.github.io/piano",
  "author": "Espen StrÃ¸mland",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite --base=/piano/",
    "prebuild": "npm run clean",
    "build": "tsc -b && vite build --base=/piano/",
    "preview": "vite preview --port 5050 --base=/piano/",
    "lint": "eslint 'src/**/*{.ts,.tsx}'",
    "lint:fix": "eslint --fix 'src/**/*{.ts,.tsx}'",
    "format": "prettier src --write",
    "clean": "rimraf build"
  },
  "dependencies": {
    "@stromland/piano-lib": "../piano-lib",
    "classnames": "2.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/jest": "29.5.14",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/webmidi": "2.1.0",
    "@vitejs/plugin-react": "4.3.3",
    "eslint": "9.14.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "globals": "15.12.0",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.14.0",
    "vite": "5.4.11"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "singleQuote": true,
    "importOrder": [
      "^react/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
