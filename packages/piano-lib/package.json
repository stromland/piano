{
  "name": "@stromland/piano-lib",
  "version": "0.4.2",
  "description": "",
  "author": "Espen Str√∏mland",
  "license": "MIT",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib",
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "run-s -l clean lint",
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "clean": "rimraf lib *.tgz coverage"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.17.6",
    "@vitest/coverage-v8": "2.1.5",
    "eslint": "9.14.0",
    "globals": "15.12.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.14.0",
    "vitest": "2.1.5"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "singleQuote": true,
    "importOrder": [
      "^react/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
