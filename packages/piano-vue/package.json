{
  "name": "@stromland/piano-vue",
  "version": "0.4.2",
  "homepage": "http://stromland.github.io/piano",
  "author": "Espen Str√∏mland",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite --base=/piano/",
    "build": "vue-tsc -b && vite build --base=/piano/",
    "preview": "vite preview --port 5050 --base=/piano/",
    "format": "prettier src --write",
    "lint": "eslint"
  },
  "dependencies": {
    "@stromland/piano-lib": "../piano-lib",
    "vue": "3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.17.6",
    "@vitejs/plugin-vue": "5.2.0",
    "@vue/tsconfig": "0.6.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "9.31.0",
    "prettier": "3.3.3",
    "typescript": "5.6.3",
    "typescript-eslint": "8.14.0",
    "vite": "5.4.11",
    "vue-tsc": "2.1.10"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "singleQuote": true,
    "importOrder": [
      "^react/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
